<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <settings>
     <setting name="cacheEnabled" value="true" />
     <setting name="mapUnderscoreToCamelCase" value="true" />
     
     <!-- MyBatis가 사용할 로깅 프레임워크 설정 -->
     <setting name="logImpl" value="LOG4J" />
  </settings>
  <!--  별칭등록 -->
  <typeAliases>
	  <typeAlias alias="bbsAdmin" type="net.musecom.comunity.model.BbsAdmin" />
	  <typeAlias alias="bbsCategory" type="net.musecom.comunity.model.BbsCategory" />
	  <typeAlias alias="bbs" type="net.musecom.comunity.model.Bbs" />
	  <typeAlias alias="member" type="net.musecom.comunity.model.Member" />
	  <typeAlias alias="memberRole" type="net.musecom.comunity.model.MemberRole" />
	  <typeAlias alias="fileDto" type="net.musecom.comunity.model.FileDto" />
  </typeAliases>
  
</configuration>